{
  "name": "dispo-api",
  "version": "0.0.1",
  "description": "Back-end API for dispo app",
  "engines": {
    "node": "8.6.0"
  },
  "main": "index.js",
  "scripts": {
    "build:ui": "cd ./ui && yarn && yarn build",
    "dev:ui": "cd ./ui && yarn && yarn start",
    "dev": "concurrently --kill-others \"npm run dev:ui\" \"nodemon --exec \"babel-node\" --watch src/ ./src/server.js\"",
    "prestart": "babel src/ -d build/",
    "start": "node build/server.js",
    "test": "node test.js"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "express": "4.15.2",
    "fs-nextra": "^0.3.2",
    "ioredis": "^3.1.4",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "flow-bin": "^0.56.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "prettier": "^1.7.4",
    "request": "^2.81.0",
    "tape": "^4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chipp972/dispo-api"
  },
  "license": "MIT"
}
