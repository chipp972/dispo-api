{
  "name": "dispo-api",
  "version": "0.0.1",
  "description": "Back-end API for dispo app",
  "engines": {
    "node": "8.7.0"
  },
  "main": "index.js",
  "scripts": {
    "dev:ui": "cd ./ui && yarn start",
    "dev:api": "nodemon --exec \"babel-node\" --watch src/ ./src/server.js",
    "dev": "nf start devapi devui",
    "test": "node test.js",
    "postinstall": "node postinstall.js && babel src/ -d build/",
    "start": "nf start web"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "1.18.2",
    "express": "4.15.2",
    "foreman": "2.0.0",
    "helmet": "3.9.0",
    "ioredis": "3.1.4",
    "ioredis-mock": "3.1.1",
    "mkdirp-promise": "5.0.1",
    "morgan": "1.9.0",
    "passport": "0.4.0",
    "ramda": "0.25.0",
    "socket.io": "^2.0.3",
    "winston": "2.4.0",
    "winston-daily-rotate-file": "1.7.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "concurrently": "^3.5.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.56.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "prettier": "^1.7.4",
    "request": "^2.81.0",
    "tape": "^4.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chipp972/dispo-api"
  },
  "license": "MIT"
}
