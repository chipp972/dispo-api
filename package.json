{
  "name": "dispo-api",
  "version": "2.0.4",
  "description": "Back-end API for dispo app",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rm -rf lib/ build/",
    "format": "prettier --write '{src,public}/**/*.js'",
    "lint": "eslint --fix src/ public/",
    "typecheck": "flow",
    "build:app": "babel src/ -d build/",
    "build:lib": "babel public/ -d lib/ && flow-copy-source public/ lib/",
    "dev": "nf start devapi",
    "pretest": "npm run format && npm run lint && npm run typecheck",
    "test": "tape --require babel-register 'src/**/__tests__/*' | tap-spec",
    "test:watch": "nf start test",
    "postinstall": "npm run build:lib && npm run build:app",
    "prestart": "npm run build:app",
    "start": "nf start web"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "dependencies": {
    "@google/maps": "0.4.5",
    "another-express-crud": "1.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "bcrypt": "1.0.3",
    "body-parser": "1.18.2",
    "cloudinary": "1.10.0",
    "cors": "2.8.4",
    "crud-mongoose-connector": "0.2.2",
    "customizable-error": "0.1.3",
    "express": "4.16.2",
    "express-form-data": "2.0.1",
    "flow-copy-source": "1.3.0",
    "foreman": "2.0.0",
    "helmet": "3.11.0",
    "jsonwebtoken": "8.1.1",
    "leaked-handles": "5.2.0",
    "mockgoose": "7.3.5",
    "moment": "2.20.1",
    "moment-timezone": "0.5.14",
    "mongoose": "5.0.5",
    "mongoose-plugin-events": "1.2.3",
    "morgan": "1.9.0",
    "nodemailer": "4.4.2",
    "nodemailer-smtp-transport": "2.7.4",
    "passport": "0.4.0",
    "passport-jwt": "3.0.1",
    "ramda": "0.25.0",
    "shortid": "2.2.8",
    "singleton-module-registry": "0.1.1",
    "socket.io": "2.0.4",
    "socket.io-client": "2.0.4",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "eslint": "4.17.0",
    "eslint-config-google": "0.9.1",
    "eslint-plugin-flowtype": "2.44.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.6.1",
    "flow-bin": "0.65.0",
    "husky": "0.15.0-rc.6",
    "nodemon": "1.14.12",
    "prettier": "1.10.2",
    "sinon": "4.3.0",
    "supertest": "3.0.0",
    "tap-spec": "4.1.1",
    "tape": "^4.7.0",
    "tape-watch": "2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chipp972/dispo-api"
  },
  "license": "MIT"
}
