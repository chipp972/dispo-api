// @flow
import { Document } from 'mongoose';

export type CompanyData = {
  owner: string,
  name: string,
  type: string,
  siret?: string,
  imageUrl?: string,
  address: string,
  phoneNumber?: string
};

export interface Company extends CompanyData, Document {
  _id: string;
  geoAddress: { latitude: number, longitude: number };
  recentlyAvailable: Date;
}
